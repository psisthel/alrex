USER FUNCTION PH02_PRIMERHUECO()

LNRESULT :=0

IF V_ANCHO1>0
	ANCHO := MIN(V_ANCHO - V_ANCHO1 , V_ANCHO1)
ELSE
	ANCHO := V_ANCHO
ENDIF

DO CASE
	CASE ANCHO < 0.245
		PERFORACION := 40
	CASE ANCHO < 0.305
		PERFORACION := 60
	CASE ANCHO < 0.405
		PERFORACION := 90
	CASE ANCHO < 0.500
		PERFORACION := 125
	OTHERWISE
		DO CASE
			CASE V_LAM == "16M" 
				PERFORACION := 125
			OThERWISE
				PERFORACION := 150
		ENDCASE
ENDCASE

LNRESULT := PERFORACION

RETURN (LNRESULT)